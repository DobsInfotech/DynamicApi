

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <style>
        table {
          font-family: arial, sans-serif;
          width: 100%;
        }

        td, th {
          text-align: left;
          padding: 8px;
        }

    </style>

</head>
<body>

    <table>

        <tr>

          <th>App Name</th>
          <th>Update data</th>
          <th>Get data</th>

        </tr>

        <% for(var i=0; i<OriginalData.length; i++) {%>
            <tr>
              <td><%= OriginalData[i].Data.App[0]%></td>
                <td><a style="text-decoration: none; color: black;" href=<%= OriginalData[i].Data.App[0]%>>Update <%= OriginalData[i].Data.App[0]%> Upi </a></td>
                <td><a style="text-decoration: none; color: black;" href=<%= OriginalData[i].Data.App[0]%>/data>Get <%= OriginalData[i].Data.App[0]%> Upi </a></td>
            </tr>
        <% } %>

      </table>

</body>
</html>
